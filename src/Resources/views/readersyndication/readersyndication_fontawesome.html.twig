{% if links|default() %}
    {% set iconClass = {
        'facebook' : 'fa fa-facebook-f',
        'twitter' : 'fa fa-twitter',
        'googleplus' : 'fa fa-google-plus',
        'linkedin' : 'fa fa-linkedin',
        'xing' : 'fa fa-xing',
        'mail' : 'fa fa-envelope-o',
        'tumblr' : 'fa fa-tumblr',
        'pinterest' : 'fa fa-pinterest-p',
        'reddit' : 'fa fa-reddit-alien',
        'whatsapp' : 'fa fa-whatsapp',
    } %}
    <ul class="syndication-list font-awesome">
        {% for key,link in links %}
            <li class="{{ 'syndication-item ' ~ link.cssClass|trim }}">
                <a href="{{ link.href }}" class="{{ ('syndication-link ' ~ link.cssClass ~ ' ' ~ (iconClass[key]|default() ? iconClass[key] :  ''))|trim }}"{% if link.target|default() %} target="{{ link.target }}"{% endif %}{% if link.name|default() %} name="{{ link.name }}"{% endif %}{% if link.rel|default() %} rel="{{ link.rel }}"{% endif %}{% if link.title|default() %} title="{{ link.title|trans }}"{% endif %}{% if link.onclick|default() %} onclick="{{ link.onclick }}"{% endif %}{% for key,attribute in link.attributes %}{{ ' ' ~ key ~ '=' ~ attribute  }}{% endfor %}><span class>{{ (iconClass[key]|default() ? '' :  link.content|trans) }}</span></a>
            </li>
        {% endfor %}
    </ul>
{% endif %}